#!/usr/bin/env node
var
  dbus= require( "dbus-native"),
  hello= require("./msg/hello"),
  List= require("./List")

var
  conn= dbus.createConnection()

conn.message(hello())

var l= List( conn, "org.PulseAudio1")
l.subscribe(function(i){
	console.log("I", i)
}, console.error.bind(null, "ohhat"), console.log.bind(null, "done"))
